export CUDA_VISIBLE_DEVICES="4,5,7"
data=/usr/atom_ai/data/oxide_pertiltpattern_1deg_GP_train_85x120.tfrecords
net=network_ResNet_18.json
hyp=hyper_params_classifier_VGG16.json
model=ResNet
batch=32
gpus=3
mode=train
resize=224
cpuid=1
checkpt=$model-batch-$batch-gpus-$gpus-imsize-$resize-$mode
echo python stemdl_classify.py --data_path $data --checkpt_dir $checkpt --network_config $net --hyper_params $hyp --num_gpus $gpus --batch_size $batch --mode $mode --resize_pixels $resize --cpu_id $cpuid
python stemdl_classify.py --data_path $data --checkpt_dir $checkpt --network_config $net --hyper_params $hyp --num_gpus $gpus --batch_size $batch --mode $mode --resize_pixels $resize --cpu_id $cpuid
# rm -r $checkpt

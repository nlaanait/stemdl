export CUDA_VISIBLE_DEVICES="7"
cp -r /usr/atom_ai/stemdl/stemdl .
cp /usr/atom_ai/stemdl/scripts/stemdl_classify.py .
data=/usr/atom_ai/data/oxide_pertiltpattern_GP_test_85x120.tfrecords
checkpt=VGG
net=network_VGG16.json
hyp=hyper_params_classifier_VGG16.json
batch=256
gpus=1
mode=eval
python stemdl_classify.py --data_path $data --checkpt_dir $checkpt --network_config $net --hyper_params $hyp --num_gpus $gpus --batch_size $batch --mode $mode
